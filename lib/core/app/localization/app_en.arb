{
    "@@locale": "en",
    "appDescription": "Firmwares for Huami wearables",
    "@appDescription": {
        "description": "App description displayed in app drawer"
    },
    "appDeveloper": "App developer",
    "@appDeveloper": {
        "description": "The message displayed in drawer item subtitle"
    },
    "apiDeveloper": "Back-End API developer",
    "@apiDeveloper": {
        "description": "The message displayed in drawer item subtitle"
    },
    "firmwareLatestMsg": "This firmware is up to date",
    "@firmwareLatestMsg": {
        "description": "The message displayed on the firmware card if it is the latest"
    },
    "firmwareUnsuitableMsg": "The preferred language isn't supported",
    "@firmwareUnsuitableMsg": {
        "description": "The message displayed on the firmware card if it does not support the default language of the current device"
    },
    "firmwareDownloadMsg": "Downloading firmware: {firmwareVersion}",
    "@firmwareDownloadMsg": {
        "description": "The message displayed when the firmware download starts",
        "placeholders": {
            "firmwareVersion": {
                "type": "String",
                "example": "1.2.3.45"
            }
        }
    },
    "firmwareLoadingErrorMsg": "Firmwares are unavailable",
    "@firmwareLoadingErrorMsg": {
        "description": "Error message displayed when firmwares are unavailable"
    },
    "retry": "Retry",
    "@retry": {
        "description": "Button title displayed when firmwares are unavailable"
    },
    "navigationErrorMsg": "Page not found",
    "@navigationErrorMsg": {
        "description": "Error message displayed when a navigation error occurrs"
    },
    "navigateBack": "Go back",
    "@navigateBack": {
        "description": "Button title displayed when a navigation error occurrs"
    }
}